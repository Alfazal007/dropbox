generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Metadata {
  id             Int      @id @default(autoincrement())
  user_id        Int
  file_path      String
  version_number Int      @default(0)
  hashes         String[]
  uploaded       Boolean  @default(false)
}
